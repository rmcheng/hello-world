git status
git status -s
git diff
  changed but not staged
git diff --staged
  staged changes

git rm
git mv

git log
git log --stat
git log --pretty=oneline
git log -S function_name

git commit --amend
git reset
  unstage a file

git remote
git remote show remote_name

git remote rename old_remote_name new_remote_name
git remote add remote_name git@github.com:rmcheng/hello-world.git
git remote remove remote_name

git push remote_name master
git fetch remote_name

git branch testing
  create new branch
git checkout testing
  switch to branch "testing
git checkout -b testing
  create new branch and switch to it
git push remote_name testing
  pushes branch into remote

git checkout master
git merge testing
  merges testing with master

git branch --list

git status 
  for merge conflicts, will show differences between branches in file